{
  "name": "synagogue.church",
  "version": "1.0.0",
  "description": "Static site generated with Eleventy (11ty)",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss src/assets/styles.css -o src/assets/styles.generated.css",
    "watch:css": "postcss src/assets/styles.css -o src/assets/styles.generated.css --watch",
    "build": "npm run build:css && eleventy",
    "serve": "eleventy --serve --pathprefix=''",
    "start": "npm-run-all --parallel watch:css serve"
  },
  "author": "joshualinog",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "luxon": "^3.7.2"
  }
}
